<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="cs">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="elemental2.core.Core"/>
    <inherits name="elemental2.dom.Dom"/>
    <inherits name="elemental2.promise.Promise"/>
    <inherits name="ol.GwtOL"/>
    <source path="client"/>
    <source path="shared"/>
    <generate-with
            class="cn.cangling.cs.server.gwt.CompileVersionGenerator">
        <when-type-assignable
                class="cn.cangling.cs.client.version.ICompileInfoProvider"/>
    </generate-with>
    <entry-point class='cn.cangling.cs.client.CsEntry'/>
</module>